{
    "name": "sweelix/yii2-redis-rbac",
    "description": "PHP 5.6+ Redis / RBAC integration for the Yii framework",
    "keywords": ["yii2", "RBAC", "redis"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Philippe Gaultier",
            "email": "pgaultier@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-redis": "~2.0",
        "php" : ">=5.6.0",
        "sweelix/guid": "~1.0"
    },
    "require-dev" : {
        "php" : ">=5.6.0",
        "codeception/codeception": "~2.2"
    },
    "autoload": {
        "psr-4": { "sweelix\\rbac\\redis\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-devel": "1.0.x-dev"
        }
    }
}
